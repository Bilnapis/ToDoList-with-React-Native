{
  "expo": {
    "name": "Ngelist",
    "slug": "ToDoListApps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconAppsCircle.png",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "contain",
      "flex": 0.5,
      "backgroundColor": "#3f51b5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ngelist.abillionaire.me"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
